<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="fr.inria.modeling.graphml.utils.objectContribution1"
            nameFilter="*.xmi"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="fr.inria.modeling.graphml.utils.menu"
               label="GraphML"
               path="additions">
            <groupMarker
                  name="group1">
            </groupMarker>
         </menu>
         <action
               class="fr.inria.modeling.graphml.utils.popup.actions.GraphMLModel2XMLModelAction"
               enablesFor="1"
               id="fr.inria.modeling.graphml.utils.action1"
               label="From GraphML model to XML model"
               menubarPath="fr.inria.modeling.graphml.utils.menu/group1">
         </action>
      </objectContribution>
      <objectContribution
            id="fr.inria.modeling.graphml.utils.objectContribution2"
            nameFilter="*.xmi"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="fr.inria.modeling.graphml.utils.menu"
               label="GraphML"
               path="additions">
            <groupMarker
                  name="group1">
            </groupMarker>
         </menu>
         <action
               class="fr.inria.modeling.graphml.utils.popup.actions.GraphMLModel2XMLFileAction"
               enablesFor="1"
               id="fr.inria.modeling.graphml.utils.action2"
               label="From GraphML model to XML file"
               menubarPath="fr.inria.modeling.graphml.utils.menu/group1">
         </action>
      </objectContribution>
   </extension>

</plugin>
